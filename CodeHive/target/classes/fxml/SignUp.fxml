<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.SignUpController">

    <VBox alignment="CENTER" layoutX="146.0" layoutY="100.0" prefHeight="332.0" prefWidth="309.0" spacing="20">
        <TextField fx:id="txtNewUsername" maxWidth="200" promptText="New Username" />
        <PasswordField fx:id="txtNewPassword" maxWidth="200" promptText="New Password" />
        <TextField fx:id="txtNewEmail" maxWidth="200" promptText="Email" />
        <Button maxWidth="100" onAction="#handleSignUp" text="Sign Up" />
        <Button fx:id="btnReturn" mnemonicParsing="false" onAction="#handleReturnButton" prefHeight="25.0" prefWidth="27.0">
        </Button>
        <Label fx:id="lblNewUserStatus" textFill="red" />
    </VBox>
</AnchorPane>
